{
    "builds": [
      {
        "src": "miniwallpapers/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.12.1"
      }
    }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "miniwallpapers/wsgi.py"
      }
    ]
  }